{
  "env": {
    "es6": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier", "import", "jest"],
  "rules": {
    "import/no-unresolved": [0],
    "import/prefer-default-export": [0],
    "no-underscore-dangle": [0]
  },
  "parser": "babel-eslint"
}
